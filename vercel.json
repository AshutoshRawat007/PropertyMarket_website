{
  "version": 2,
  "builds": [
    {
      "src": "/api/*",
      "use": "@vercel/node"
    },
    {
      "src": "/client/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/client/(.*)",
      "dest": "/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/src/index.jsx"
    }
  ]
}
